{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "All Devices (Canvaskit)",
            "request": "launch",
            "type": "dart",
            "deviceId": "all",
            "args": [
                "--no-sound-null-safety",
                "--web-renderer",
                "canvaskit",
                "--web-hostname=127.0.0.1",
                "--web-port=4000"
            ],
        },
        {
            "name": "Web Canvaskit",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "args": [
                "--no-sound-null-safety",
                "--web-renderer",
                "canvaskit",
                "--web-hostname=127.0.0.1",
                "--web-port=4000"
            ]
        },
        {
            "name": "Web HTML",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "args": [
                "--no-sound-null-safety",
                "--web-renderer",
                "html",
                "--web-hostname=127.0.0.1",
                "--web-port=4000"
            ]
        },
        {
            "name": "Web Canvaskit Performance Profilinf",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "flutterMode": "profile",
            "args": [
                "--no-sound-null-safety",
                "--web-renderer",
                "canvaskit",
                "--web-hostname=127.0.0.1",
                "--web-port=4000",
            ]
        },
        {
            "name": "Web HTML Performance Profilinf",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "flutterMode": "profile",
            "args": [
                "--no-sound-null-safety",
                "--web-renderer",
                "html",
                "--web-hostname=127.0.0.1",
                "--web-port=4000",
            ]
        },
        {
            "name": "Web Canvaskit Experimental Text Release",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "flutterMode": "release",
            "args": [
                "--no-sound-null-safety",
                "--web-renderer",
                "canvaskit",
                "--web-hostname=127.0.0.1",
                "--web-port=4000",
                "--dart-define=FLUTTER_WEB_USE_EXPERIMENTAL_CANVAS_TEXT=true",
            ]
        },
    ]
}